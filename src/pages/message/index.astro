---
import IndexPage from "src/layouts/IndexPage.astro";

let posts = await Astro.glob("./message.md");
let {Content, frontmatter} = posts[0];


---

<IndexPage frontmatter={frontmatter}>
  <article class="markdown-body">
    <Content/>
  </article>

<script data-letterbirduser="ghastlymirror" src="https://letterbird.co/embed/v1.js"></script>
</IndexPage>

